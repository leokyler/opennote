{
  "name": "@repo/opennote",
  "version": "0.1.0",
  "description": "OpenNote initialization commands for OpenCode CLI - install predefined note templates with automatic registration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "opennote": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "exports": {},
  "keywords": [
    "opennote",
    "opencode",
    "note-taking",
    "cli",
    "template",
    "markdown",
    "productivity",
    "zettelkasten"
  ],
  "author": "OpenNote Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/opennote.git"
  },
  "homepage": "https://github.com/yourusername/opennote#readme",
  "bugs": {
    "url": "https://github.com/yourusername/opennote/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "@commander-js/extra-typings": "^11.1.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "tsx": "^4.7.0",
    "@repo/eslint-config": "workspace:*"
  }
}
